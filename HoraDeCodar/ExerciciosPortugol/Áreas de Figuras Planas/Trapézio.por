programa {
  funcao inicio() {
    inteiro basemaior
    inteiro basemenor
    inteiro h

    escreva ("Qual o tamanho da base maior do seu Trapézio? ")
    leia (basemaior)

    escreva ("Qual o tamanho da base menor do seu Trapézio? ")
    leia (basemenor)

        escreva ("Qual a altura do seu Trapézio? ")
    leia (h)

    inteiro trapezio = (basemaior + basemenor) * h / 2
        escreva("A área do Trapézio é de: ", trapezio)
  }
}
